{
  "manifest_version": 2,

  "name": "Zero-tags",
  "description": "This extension help set tags on pictures onto Zerochan",
  "version": "1.2",
  "permissions": [ "tabs","http://www.zerochan.net/", "contextMenus" ],
  "icons": {
    "16": "Scroll.png"},
  "background": {
    "scripts": ["popup.js"]	},
  "browser_action": {
    "default_icon": "Scroll.png",
    "default_popup": "popup.html"
  }
}